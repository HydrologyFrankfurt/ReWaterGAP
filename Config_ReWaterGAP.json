{
  "FilePath": {
    "inputDir": {"climate_forcing": "input_data/climate_forcing/",
                 "water_use_data": "input_data/water_use/",
                 "static_land_data": "input_data/static_input/"},
                
    "outputDir": "output_data/"
  },
  "RuntimeOptions": [
    {
      "SimulationOption": {
        "AntNat_opts": {"ant": true,  "subtract_use": true, "res_opt": true},
        "Demand_satisfaction_opts": {"delayed_use": true, "neighbouring_cell": true}
      }
    },
    {
    "RestartOptions":{
        "restart": false,
        "save_model_states_for_restart": true,
        "save_and_read_states_dir": "./"
        }
    },
    {
      "SimulationPeriod": {
        "start": "1990-01-01",
        "end": "1990-12-31",
        "reservoir_start_year": 1990,
        "reservoir_end_year": 2019,
        "spinup_years": 5
      }
    },
    {
      "TimeStep": {
        "daily": true
      }
    },
    {
    "SimulationExtent": { 
     "run_basin": false,
     "path_to_stations_file": "input_data/static_input/"}
     }
  ],
  "OutputVariable": [
    {
      "VerticalWaterBalanceFluxes": {
        "pot_evap":false,
        "net_rad":false,
        "leaf_area_index":false,
        "canopy_evap":false,
        "throughfall":false,
        "snow_fall":false,
        "snow_melt":false,
        "snow_evap":false,
        "groundwater_recharge":false,
        "surface_runoff":false
      }
    },
    {
      "VerticalWaterBalanceStorages": {
        "canopy_storage":false,
        "snow_water_equiv":false,
        "soil_moisture":false
      }
    },
    {
      "LateralWaterBalanceFluxes": {
        "groundwater_discharge":false,
        "local_lake_outflow":false,
        "local_wetland_outflow":false,
    	"global_lake_outflow":false,
        "global_wetland_outflow":false,
        "streamflow": false,
        "actual_net_abstr_groundwater": false,
         "demand_satisfied_by_cell":false,
         "total_demand_into_cell":false,
         "unsat_potnetabs_sw_from_demandcell":false,
         "returned_demand_from_supply_cell": false,
         "prev_returned_demand_from_supply_cell":false,
         "total_unsatisfied_demand_ripariancell": false,
         "accumulated_unsatisfied_potential_netabs_sw":false,
         "get_neighbouring_cells_map": false,
         "total_unsatisfied_demand_from_supply_to_all_demand_cell": false
      }
    },
    {
      "LateralWaterBalanceStorages": {
        "groundwater_storage":false,
        "local_lake_storage": true,
        "local_wetland_storage":false,
    	"global_lake_storage": true,
    	"global_wetland_storage":false,
    	"river_storage": false,
    	"global_reservoir_storage": true
      }
    }
  ]
}